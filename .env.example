# Copy this file to config/.env and fill in your values

# OpenRouter (Recommended - gives access to multiple models)
OPENROUTER_API_KEY=

# OpenAI (Optional)
OPENAI_API_KEY=

# Anthropic (Optional)
ANTHROPIC_API_KEY=

# Default LLM Provider (openrouter, openai, anthropic, ollama)
DEFAULT_LLM_PROVIDER=openrouter
DEFAULT_MODEL=anthropic/claude-3.5-sonnet
